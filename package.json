{
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "browser-sync": "^3.0.2",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.0",
    "vercel": "^33.0.1"
  },
  "scripts": {
    "dev": "npm-run-all --parallel rebuild-on-change refresh-on-change",
    "build": "bb build.bb",
    "rebuild-on-change": "nodemon -e clj --watch src/ --exec \"bb build.bb\"",
    "refresh-on-change": "npx browser-sync start --ss 'dist' -w"
  }
}