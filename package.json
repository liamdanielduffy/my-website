{
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "browser-sync": "^3.0.2",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "tailwindcss": "^3.4.0",
    "vercel": "^33.0.1"
  },
  "scripts": {
    "dev": "concurrently npm:rebuild npm:refresh",
    "build": "bb build.bb",
    "rebuild": "nodemon -e clj --watch src/ --exec \"bb build.bb\"",
    "refresh": "npx browser-sync start --ss 'build/final' -w"
  }
}